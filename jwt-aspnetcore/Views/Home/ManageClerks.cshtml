@using JWTASPNetCore.Models
@model List<UserDTO>
@{
    ViewData["Title"] = "Manage Clerks";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>Clerks</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>User</th>
            <th>Role</th>
            
        </tr>
    </thead>
    <tbody>
       @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Role</td>
            </tr>
        }
    </tbody>
</table>